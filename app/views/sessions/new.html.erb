<h1>Log In</h1>
<p><%= flash[:error] %></p>

<%= form_for (:user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br><br>
    
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit 'Log In' %>
    <br><br>
    <%= link_to 'Sign Up', signup_path %><br>
    <%= link_to 'Log In with Google', '/auth/google_oauth2' %>
<% end %>


