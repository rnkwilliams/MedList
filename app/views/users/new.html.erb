<div class="signup">
<h1>Sign Up!</h1>


<%= render partial: 'layouts/errors', locals: {object: @user} %>

<%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br><br>
    <%= f.label :email %>
    <%= f.email_field :email %><br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit 'Sign Up' %>
    <br><br>
    <%= link_to 'Log In', login_path %><br>
    <%= link_to 'Log In with Google', '/auth/google_oauth2' %>
<% end %>
</div>