<% if logged_in? %>
    <%= link_to 'Add Medication', new_medication_path %>
    <%= link_to 'My Profile', user_path(current_user) %>
    <%= link_to 'Log Out', logout_path, method: :delete %>

<% else %>
    <%= link_to 'Home', root_path %>
    <%= link_to 'Sign Up', signup_path %>
    <%= link_to 'Log In', login_path %>
<% end %>